{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.json",
    "noWatch": true
  },
  "extends": [
    "agoda"
  ],
  "rules": {
    "@typescript-eslint/indent": [
      "warn",
      2
    ],
    "react/jsx-indent-props": [
      "warn",
      2
    ],
    "react/jsx-indent": [
      "warn",
      2
    ],
    "arrow-parens": "off"
  }
}